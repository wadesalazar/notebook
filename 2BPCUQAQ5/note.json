    {
      "title": "Clean up, create and populate Phoenix tables for timeseries",
      "text": "%phoenix\n\ndrop view \"timeseries\"\ndrop table if exists \"sinusoid\"\n\ncreate view \"timeseries\"(\n   \n     \"cf\".\"t\"   varchar(10),\n     \"cf\".\"v\" varchar(10),\n      \"cf\".\"id\" varchar(10),\n\n     \"PKEY\" varchar(10),\n CONSTRAINT pk PRIMARY KEY (PKEY)\n)\n\n\ncreate table if not exists \"sinusoid\"(\n \"cf\".\"timestamp\" TimeStamp,\n \"cf\".\"value\" Decimal,\n \"cf\".\"metric\" varchar(10),\n \"PKEY\" varchar(50)\n CONSTRAINT pk PRIMARY KEY (PKEY)\n)\n\nupsert into \"sinusoid\" select TO_DATE(TO_CHAR(TO_NUMBER(\"t\")/1000,\u0027#\u0027),\u0027ssssssssss\u0027),TO_NUMBER(\"v\"),\"id\",\"PKEY\" from \"timeseries\"",
